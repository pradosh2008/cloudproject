. ${HOME}/lib/set_env.sh
. ${HOME}/lib/common.sh

#Unique Key CHECK

sql='
select count(*) cnt
from
    (select
    ID_CHAIN
    , ID_EMAIL
    from edl_stage.plus_vmci995_esp_last_click_open
    group by 1,2
    having count(*)>1
)'


run_validation "$sql" "Unique Key Check"

#completeness check

sql='
SELECT
    count(*) cnt
from edl_landing.lbca_vmci995_esp_last_click_open l
left join edl_stage.plus_vmci995_esp_last_click_open p
    on CAST(TRIM(l.ID_CHAIN) AS INT64) = p.ID_CHAIN
    and CAST(TRIM(l.ID_EMAIL) AS INT64) = p.ID_EMAIL
where p.ID_EMAIL is null'

run_validation "$sql" "Completeness Check"


exit ${ERROR_CD:-0}

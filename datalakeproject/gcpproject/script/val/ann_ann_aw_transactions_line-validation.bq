#!/usr/bin/bash
. ${HOME}/lib/set_env.sh
. ${HOME}/lib/common.sh

#Completeness CHECK

sql=
'select
    count(*) cnt
	from edl_landing.ann_ann_aw_transactions_line l
left join edl_stage.pre_aw_transaction_line s
    on l.if_entry_no = s.if_entry_no
  and l.line_id = s.line_id
where l.if_entry_no is null
'

run_validation "$sql" "Completeness Check"


#Unique Key CHECK
sql='
select count(*) cnt 
from
    (
    select 
        l.if_entry_no
        , l.line_id
        ,count(*) cnt
    from edl_stage.pre_aw_transaction_line l
    group by 1,2
    having count(*) >1
    )
'
run_validation "$sql" "Unique Key Check"

exit ${ERROR_CD:-0}
#!/usr/bin/bash
. ${HOME}/lib/set_env.sh
. ${HOME}/lib/common.sh

#Unique Key CHECK

sql='
select count(*) cnt 
from
    (
    select 
        h.if_entry_no
        ,count(*) cnt
    from edl_stage.pre_aw_transaction_header h
    group by 1
    having count(*) >1
    )
'

run_validation "$sql" "Unique Key Check"

#Completeness CHECK

sql='
select 
    count(*) cnt
from edl_landing.ann_ann_aw_transactions_header l
left join edl_stage.pre_aw_transaction_header s
    on l.if_entry_no = s.if_entry_no 
where l.if_entry_no is null
'

run_validation "$sql" "Completeness Check"


exit ${ERROR_CD:-0}




#Load new and updated into new table
bq query --project_id='p-asna-analytics-002' --max_rows 1 --allow_large_results --destination_table jus_stage.dim_program_new --use_legacy_sql=false <<!
SELECT c.*
from jus_stage.work_dim_program c
!
#Add legacy records from old table
bq query --project_id='p-asna-analytics-002' --max_rows 1 --allow_large_results --append_table --destination_table jus_stage.dim_program_new --use_legacy_sql=false <<!
select c.*
from jus_stage.dim_program c
left join jus_stage.work_dim_program w
    on  w.program_key = c.program_key
where w.program_key is null
!
bq cp --force jus_stage.dim_program edl_archive.dim_program
bq cp --force jus_stage.dim_program_new jus_stage.dim_program
bq rm --force jus_stage.dim_program_new

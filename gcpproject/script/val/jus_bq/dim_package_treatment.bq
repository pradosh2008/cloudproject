#Add legacy records from old table
bq query --project_id='p-asna-analytics-002' --max_rows 1 --allow_large_results --append_table --destination_table jus_stage.dim_package_treatment_new --use_legacy_sql=false <<!
select c.*
from jus_stage.dim_package_treatment c

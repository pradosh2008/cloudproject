#!/usr/bin/bash
. ${HOME}/lib/set_env.sh
. ${HOME}/lib/common.sh

#Unique Key CHECK

sql='
select count(*) cnt 
from
    (
    select 
        l.if_entry_no
        , l.line_id
        , l.line_note_type
        ,count(*) cnt
    from edl_stage.pre_aw_transaction_linenotes l
    group by 1,2,3
    having count(*) >1
    )' 
	

run_validation "$sql" "Unique Key Check"

#completeness check 

sql='
select 
    count(*) cnt
from edl_landing.ann_ann_aw_transactions_linenotes l
left join edl_stage.pre_aw_transaction_linenotes s
    on l.if_entry_no = s.if_entry_no
  and l.line_id = s.line_id
  and l.line_note_type = s.line_note_type
where l.if_entry_no is null'

run_validation "$sql" "Completeness Check"


exit ${ERROR_CD:-0}


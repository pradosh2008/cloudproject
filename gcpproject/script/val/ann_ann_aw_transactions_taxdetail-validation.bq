#!/usr/bin/bash
. ${HOME}/lib/set_env.sh
. ${HOME}/lib/common.sh

#Unique Key CHECK

sql='
select count(*) cnt 
from
    (
    select 
        t.if_entry_no
        , t.line_id
    , t.tax_level
        ,count(*) cnt
    from edl_stage.pre_aw_transaction_tax_detail t
    group by 1,2,3
    having count(*) >1
    )' 
	

run_validation "$sql" "Unique Key Check"

#completeness check 

sql='
select 
    count(*) cnt
from edl_landing.ann_aw_transactions_taxdetail l
left join edl_stage.pre_aw_transaction_tax_detail s
    on l.if_entry_no = s.if_entry_no
  and l.line_id = s.line_id
  and l.tax_level = s.tax_level
where l.if_entry_no is null'

run_validation "$sql" "Completeness Check"


exit ${ERROR_CD:-0}

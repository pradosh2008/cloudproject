#!/usr/bin/bash

. ${HOME}/lib/set_env.sh
. ${HOME}/lib/common.sh

#creating the landing table
bq load --replace=true --source_format=CSV --skip_leading_rows=1 --field_delimiter="|" \
        --schema=${schema_path}/edl_landing/vmci173_full_email_address.json \
        edl_landing.vmci173_full_email_address  \
        "gs://${default_bucket}/plus/vmci173_full_email_address_YYYYMMDD*.txt.gz"
rc_check $? "Load edl_landing"
